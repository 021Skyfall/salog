{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "standard-with-typescript","prettier/prettier", "plugin:prettier/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "indent": [2,"tab"],
    "@typescript-eslint/explicit-function-return-type" : "off",
    "react/react-in-jsx-scope": "off",
    "prettier/prettier": [
            "error",
            {
                "useTabs": true,
                "endOfLine": "auto",
                "singleQuote": false
            }
        ]
  }
}
